# Production Environment Configuration
GO_ENV=production
LOG_LEVEL=warn
VERSION=latest

# Database Configuration
# These should be set via secure methods (secrets, vault, etc.)
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=

# Redis Configuration
REDIS_PASSWORD=

# Grafana Configuration
GRAFANA_ADMIN_USER=
GRAFANA_ADMIN_PASSWORD=

# Monitoring Configuration
METRICS_ENABLED=true
HEALTH_CHECK_ENABLED=true
CERTIFICATE_WATCHER_ENABLED=true

# Docker Configuration
DOCKER_REGISTRY=ghcr.io
IMAGE_NAME=tlsai-agent
